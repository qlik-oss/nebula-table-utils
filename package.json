{
  "name": "@qlik-oss/nebula-table-utils",
  "license": "MIT",
  "author": "QlikTech International AB",
  "version": "0.0.1",
  "description": "Common Components, Hooks and utils between `sn-table` and `sn-pivot-table` projects",
  "keywords": [
    "sn-table",
    "sn-pivot-table",
    "common"
  ],
  "type": "module",
  "main": "lib/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "npm": ">=9",
    "node": ">=18.16.0"
  },
  "files": [
    "./lib/**",
    "!**/bundle-analyzer-report.html",
    "!**/README.md",
    "!**/LICENSE",
    "!**/package.json",
    "./README.md",
    "./LICENSE",
    "./package.json"
  ],
  "scripts": {
    "clear": "rimraf ./lib/*",
    "build:prod": "npm run clear & rollup -c",
    "build:watch": "nodemon --exec ./build-utils/build-copy.sh",
    "lint": "eslint src/*",
    "format:check": "prettier --check './src/**/*.{ts,tsx}' --ignore-unknown",
    "types:check": "tsc --noEmit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') console.log(e);}\""
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@qlik/eslint-config": "^0.4.8",
    "@qlik/eslint-config-react": "^0.1.0",
    "@qlik/prettier-config": "^0.2.0",
    "@qlik/tsconfig": "^0.1.4",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.4",
    "@swc/core": "^1.3.69",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.14",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.44.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.26.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.4.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-swc3": "^0.9.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "semantic-release": "^21.0.7",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@1.22.19",
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
