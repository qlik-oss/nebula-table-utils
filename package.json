{
  "name": "@qlik-oss/nebula-table-utils",
  "license": "MIT",
  "author": "QlikTech International AB",
  "version": "1.0.1-4.gc91b8f6",
  "description": "Common Components, Hooks and utils between `sn-table` and `sn-pivot-table` projects",
  "keywords": [
    "sn-table",
    "sn-pivot-table",
    "common"
  ],
  "main": "dist/index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "npm": ">=9",
    "node": ">=18.16.0"
  },
  "files": [
    "/components",
    "/hooks",
    "/utils",
    "/*.js",
    "/*.js.map",
    "/*.d.ts"
  ],
  "scripts": {
    "clear": "rimraf ./dist/*",
    "build:prod": "npm run clear & webpack -- --mode production --progress",
    "build:watch": "nodemon --exec ./build-utils/build-copy.sh",
    "build:prod:analyze": "npm run clear & webpack -- --mode production --progress --env presets.analyze",
    "lint": "eslint src/*",
    "format:check": "prettier --check './src/**/*.{ts,tsx}' --ignore-unknown",
    "types:check": "tsc --noEmit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') console.log(e);}\""
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@qlik/eslint-config": "^0.4.8",
    "@qlik/eslint-config-react": "^0.1.0",
    "@qlik/prettier-config": "^0.2.0",
    "@qlik/tsconfig": "^0.1.4",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.44.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.0",
    "prettier": "^3.0.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^21.0.7",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "packageManager": "yarn@1.22.19",
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
