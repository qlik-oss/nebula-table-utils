{
  "name": "@qlik-oss/table-common",
  "license": "MIT",
  "author": "QlikTech International AB",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "description": "Common Components, Hooks and utils between `sn-table` and `sn-pivot-table` projects",
  "keywords": [
    "sn-table",
    "sn-pivot-table",
    "common"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "engines": {
    "npm": ">=9",
    "node": ">=18.16.0"
  },
  "scripts": {
    "clear": "rimraf ./dist/*",
    "build:dev": "npm run clear & webpack-dev-server -- --mode development --progress --hot",
    "build:prod": "rollup -c",
    "build:watch": "./build-copy.sh",
    "build:prod:analyze": "npm run clear & webpack -- --mode production --progress --env presets.analyze",
    "prettier": "prettier --write './src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@qlik/eslint-config": "^0.4.8",
    "@qlik/prettier-config": "^0.1.3",
    "@qlik/tsconfig": "^0.1.4",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@swc/core": "^1.3.68",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.44.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rollup": "^3.26.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-multi-input": "^1.4.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-swc3": "^0.8.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6"
  },
  "packageManager": "yarn@1.22.19",
  "peerDependencies": {
    "@nebula.js/stardust": "^4.2.3",
    "react": "^18.2.0"
  }
}
