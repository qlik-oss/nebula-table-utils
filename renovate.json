{
  "extends": ["qlik-oss", ":dependencyDashboard"],
  "semanticCommits": true,
  "rangeStrategy": "pin",
  "packageRules": [
    {
      "depTypeList": ["engines"],
      "enabled": false
    },
    {
      "matchPackageNames": ["@qlik-trial/sprout", "qlik-chart-modules"],
      "enabled": false
    },
    {
      "rangeStrategy": "bump",
      "matchDepTypes": ["peerDependencies"]
    }
  ]
}
